    "angular-route": "^1.5.11"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "d3": "^5.5.0",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-sass": "^4.0.1",
    "http-server": "^0.11.1",
    "istanbul": "^0.4.5",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
